generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model ContactSubmission {
  id          String   @id @default(cuid())
  name        String
  email       String
  projectType String
  message     String   @db.Text
  budget      String?
  createdAt   DateTime @default(now())

  @@index([email, createdAt])
}
